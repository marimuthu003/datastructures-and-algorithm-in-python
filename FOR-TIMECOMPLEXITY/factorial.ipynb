    "def factorial_descending(n):\n",
    "    if n == 0 or n == 1:\n",
    "        result = 1\n",
    "    else:\n",
    "        result = n * factorial_descending(n - 1)\n",
    "    \n",
    "    print(f\"{n}! = {result}\")\n",
    "    return result\n",
    "\n",
    "# Example\n",
    "number = int(input(\"Enter a number: \"))\n",
    "factorial_descending(number)\n"
   ]
