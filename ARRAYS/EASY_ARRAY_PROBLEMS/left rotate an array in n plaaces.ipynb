    "def leftrotate(arr,n,k):\n",
    "    temp=[0]*n\n",
    "    for i in range(n):\n",
    "        temp[(i+k)%n]=arr[i]\n",
    "    for i in range(n):\n",
    "        print(temp[i],end=\" \")\n",
    "if __name__==\"__main__\":\n",
    "    arr=[3,5,6,1,2]\n",
    "    n=len(arr)\n",
    "    k=2\n",
    "    leftrotate(arr,n,k)"
   ]
