    "x=int(input(\"enter a number:\"))\n",
    "rev=0\n",
    "original=x\n",
    "while(x!=0):\n",
    "    dig=x%10\n",
    "    rev=rev*10+dig\n",
    "    x//=10\n",
    "print(rev)\n",
    "if original==rev:\n",
    "    print(\"palindrome number\")\n",
    "else:\n",
    "    print(\"not a palindrome number\")\n"
